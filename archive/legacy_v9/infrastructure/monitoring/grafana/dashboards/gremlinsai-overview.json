{
  "dashboard": {
    "id": null,
    "title": "DRYAD.AI System Overview",
    "tags": ["DRYAD.AI", "ai", "llm", "agents"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health Score",
        "type": "stat",
        "targets": [
          {
            "expr": "DRYAD.AI_health_score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "LLM Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(DRYAD.AI_llm_requests_total[5m])",
            "refId": "A",
            "legendFormat": "{{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Active AI Components",
        "type": "stat",
        "targets": [
          {
            "expr": "DRYAD.AI_llm_active_requests",
            "refId": "A",
            "legendFormat": "LLM Requests"
          },
          {
            "expr": "DRYAD.AI_agent_active_workflows",
            "refId": "B",
            "legendFormat": "Agent Workflows"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 4,
        "title": "LLM Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(DRYAD.AI_llm_request_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(DRYAD.AI_llm_request_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Agent Workflow Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(DRYAD.AI_agent_workflows_total{status=\"success\"}[5m]) / rate(DRYAD.AI_agent_workflows_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "DRYAD.AI_system_cpu_usage_percent",
            "refId": "A",
            "legendFormat": "CPU %"
          },
          {
            "expr": "DRYAD.AI_system_memory_usage_percent",
            "refId": "B",
            "legendFormat": "Memory %"
          }
        ],
        "yAxes": [
          {
            "label": "Percent",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 8}
      },
      {
        "id": 7,
        "title": "Model Performance",
        "type": "table",
        "targets": [
          {
            "expr": "DRYAD.AI_llm_tokens_per_second",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(DRYAD.AI_llm_errors_total[5m])",
            "refId": "A",
            "legendFormat": "LLM Errors"
          },
          {
            "expr": "rate(DRYAD.AI_agent_workflows_total{status=\"error\"}[5m])",
            "refId": "B",
            "legendFormat": "Agent Errors"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1
  }
}
